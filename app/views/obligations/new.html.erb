<div class="text-center">
  <h1>New Obligation</h1>
  <p>Who will join the fight and help in battle!</p>
</div>
<div class="container">
  <%= simple_form_for [@character, @obligation] do |f| %>
    <%= f.association :character %>
    <%= f.association :battle %>
    <%= f.submit class: 'btn btn-primary mt-3' %>
  <% end %>
</div>
