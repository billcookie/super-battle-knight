<h1>New Character</h1>
<p>Who will you add to join the fight?</p>

<div class="container">
  <%= simple_form_for(@character) do |f| %>
    <%= f.input :name %>
    <%= f.input :age %>
    <%= f.input :ranking %>
    <%= f.input :bio %>
    <%= f.input :photo, as: :file %>
    <%= f.submit class: 'btn btn-primary mt-3' %>
  <% end %>
</div>
